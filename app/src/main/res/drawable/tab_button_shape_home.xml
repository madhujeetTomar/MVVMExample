<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="#292929"/>
            <gradient android:startColor="#292929" android:endColor="#292929" android:angle="-90"/>
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="3dp"/>
            <stroke android:width="1dp" android:color="#414342"/>
            <gradient android:startColor="#414342" android:endColor="#414342" android:angle="-90"/>
        </shape>
    </item>
</selector>
